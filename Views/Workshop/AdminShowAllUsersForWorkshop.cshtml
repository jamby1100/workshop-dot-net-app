@model OneWorkshopView

<br>
<br>

<table class="table">
  <thead>
    <tr>
      <th scope="col">UserId</th>
      <th scope="col">WorkshopId</th>
    </tr>
    <tbody>
        @foreach (var row in @Model.WorkshopProgressList) {
            <tr>
                <td> 
                     <a href="/admin/workshops/@row.WorkshopId/users/@row.UserId" class="card-link">
                        @row.UserId
                     </a>
                </td>
                <td> @row.WorkshopId </td>
                <td> </td>
            </tr>
        }
    </tbody>
  </thead>
  <tbody>
</table>
